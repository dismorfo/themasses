.views-g {
  letter-spacing: -0.31em;
  *letter-spacing: normal;
  *word-spacing: -0.43em;
  text-rendering: optimizespeed;
}

.opera-only :-o-prefocus,
.views-g {
  word-spacing: -0.43em;
}

.views-u {
  display: inline-block;
  zoom: 1; *display: inline;
  letter-spacing: normal;
  word-spacing: normal;
  vertical-align: top;
  text-rendering: auto;
}

.views-u-1,.views-u-1-2,.views-u-1-3,.views-u-2-3,.views-u-1-4,.views-u-3-4,.views-u-1-5,.views-u-2-5,.views-u-3-5,.views-u-4-5,.views-u-1-6,.views-u-5-6 {
  display: inline-block;
  zoom: 1; *display: inline; 
  letter-spacing: normal;
  word-spacing: normal;
  vertical-align: top;
  text-rendering: auto;
}

.views-u-1 {
  display:block;
}

  .views-u-1-2 {
    width: 50%;
  }

  .views-offset-1-2 {
    margin-left: 50%;
  }
  
  .views-u-1-3 {
    width: 33.33333%;
  }

  .views-offset-1-3 {
    margin-left: 33.33333%;
  }
  
  .views-u-2-3 {
    width: 66.66667%;
  }

  .views-offset-2-3 {
    margin-left: 66.66667%;
  }
  
  .views-u-1-4 {
    width: 25%;
  }

  .views-offset-1-4 {
    margin-left: 25%;
  }
  
  .views-u-3-4 {
    width: 75%;
  }

  .views-offset-3-4 {
    margin-left: 75%;
  }
  
  .views-u-1-5 {
    width: 20%;
  }

  .views-offset-1-5 {
    margin-left: 20%;
  }
  
  .views-u-2-5 {
    width: 40%;
  }

  .views-offset-2-5 {
    margin-left: 40%;
  }
  
  .views-u-3-5 {
    width: 60%;
  }

  .views-offset-3-5 {
    margin-left: 60%;
  }
  
  .views-u-4-5 {
    width: 80%;
  }

  .views-offset-4-5 {
    margin-left: 80%;
  }
  
  .views-u-1-6 {
    width: 16.66667%;
  }

  .views-offset-1-6 {
    margin-left: 16.66667%;
  }
  
  .views-u-5-6 {
    width: 83.33333%;
  }

  .views-offset-5-6 {
    margin-left: 83.33333%;
  }
  
.views-g-r {
  letter-spacing: -0.31em; 
  *letter-spacing: normal; 
  word-spacing: -0.43em; 
}

.opera-only :-o-prefocus,
  .views-g-r {
    word-spacing: -0.43em;
  }


  .views-g-r img {
  max-width: 100%;
  }


  @media (min-width:980px) {
    .views-visible-phone   { display: none; }
    .views-visible-tablet  { display: none; }
    .views-visible-desktop   { }
    .views-hidden-phone  { }
    .views-hidden-tablet   { }
    .views-hidden-desktop  { display: none; }
  }

  @media (max-width:480px) {
    .views-g-r > [class ^= "views-u"] {
    width:100%;
    }
  }
  
  @media (max-width:767px) {

    .views-g-r > [class ^= "views-u"] {
    width:100%;
    }    
    .views-visible-phone   { } 
    .views-hidden-phone  { display: none; }
    .views-hidden-desktop  { }
    .views-visible-desktop   { display: none; }
  }
  
  @media (min-width:768px) and (max-width:979px) {
    .views-visible-tablet  {}
    .views-hidden-tablet   { display: none; }
    .views-hidden-desktop  {}
    .views-visible-desktop   { display: none; }
  }